[singws]
version = 0.1.0
path = /home/sloretz/ws/noetic
container = /home/sloretz/ws/noetic/ros.focal.sandbox

[ref]
_shell = singularity shell --no-home --bind "${singws:path}/src" --pwd "${singws:path}"

[commands]
shell = ${ref:_shell} --nv ${singws:container}
rootshell = ${ref:_shell} --fakeroot ${singws:container}
